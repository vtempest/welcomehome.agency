{
  "name": "repliers-opensource-portal-frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "cross-env NODE_OPTIONS='--inspect' next dev",
    "clean": "rimraf .next .open-next",
    "build": "npm run clean && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "prettier": "prettier --check ./**/*.{ts,tsx,js}",
    "prettier:fix": "prettier  --write ./**/*.{ts,tsx,js}",
    "eslint": "eslint \"./**/*.{ts,tsx,js}\"",
    "eslint:fix": "eslint --fix \"./**/*.{ts,tsx,js}\"",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage=false --no-watch --runInBand",
    "test:ci": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage=false --no-watch --runInBand --verbose=false --json --outputFile=./test-result.json",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --no-watch --silent",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch --verbose",
    "cloudflare:build": "opennextjs-cloudflare build",
    "cloudflare:dev": "opennextjs-cloudflare build && wrangler dev",
    "cloudflare:preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cloudflare:deploy": "opennextjs-cloudflare build && wrangler deploy",
    "cloudflare:tail": "wrangler tail"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/poppins": "^5.2.6",
    "@hookform/resolvers": "^4.1.3",
    "@mapbox/mapbox-gl-draw": "^1.5.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mui/icons-material": "^6.4.12",
    "@mui/lab": "^6.0.1-beta.35",
    "@mui/material": "^6.4.12",
    "@mui/material-nextjs": "^6.4.12",
    "@mui/system": "^6.4.12",
    "@mui/x-date-pickers": "^7.28.2",
    "@next/mdx": "^15.3.5",
    "@next/third-parties": "^15.3.5",
    "@number-flow/react": "^0.5.10",
    "@opennextjs/cloudflare": "^1.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@turf/helpers": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "@types/geojson": "^7946.0.16",
    "@types/mdx": "^2.0.13",
    "@types/polylabel": "^1.1.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "deepmerge": "^4.3.1",
    "embla-carousel-react": "^8.6.0",
    "fast-equals": "^5.2.2",
    "geojson": "^0.5.0",
    "joi": "^17.13.3",
    "js-cookie": "^3.0.5",
    "jwt-decode": "4.0.0",
    "libphonenumber-js": "^1.12.9",
    "mapbox-gl": "^3.13.0",
    "mapbox-gl-draw-freehand-mode": "^2.1.0",
    "next": "15.3.8",
    "next-intl": "^4.3.4",
    "p-throttle": "^6.2.0",
    "query-string": "^9.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "recharts": "^3.0.2",
    "sharp": "^0.33.5",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": "^v22.14.0",
    "npm": "^11.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/mapbox-gl": "^3.4.1",
    "@types/mapbox__mapbox-gl-draw": "^1.4.9",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@typescript-eslint/typescript-estree": "^8.35.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "^15.3.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslintcc": "^0.8.3",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.5.2",
    "micromatch": "^4.0.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "wrangler": "^3.0.0",
    "@cloudflare/workers-types": "^4.20231218.0"
  }
}
